{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug usage.ts",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--import",
        "tsx"
      ],
      "args": [
        "${workspaceFolder}/src/cosmos-db-nosql-nodejs-quickstart/examples/usage.ts"
