      "cwd": "${workspaceFolder}/src/cosmos-db-nosql-nodejs-quickstart",
      "envFile": "${workspaceFolder}/src/cosmos-db-nosql-nodejs-quickstart/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal"
    }
  ]
}
